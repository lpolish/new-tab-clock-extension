{
  "manifest_version": 3,
  "name": "New Tab Clock",
  "version": "1.1.0",
  "description": "Displays a digital clock and date on new tab pages.",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "options_page": "options.html"
}
